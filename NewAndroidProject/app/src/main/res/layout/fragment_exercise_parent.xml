<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rocket.biometrix.biometrix.ExerciseModule.ExerciseParent">

    <Button
        style="@style/create_entry_button"
        android:id="@+id/exNewEntry"
        android:onClick="CreateEntryOnClick" />

    <View
        style="@style/Divider"
        android:layout_below="@+id/exNewEntry"
        android:id="@+id/dividerUpper"
        />

    <ScrollView style="@style/scrollview_parent_list"
        android:id="@+id/sleepDisplayEntriesScrollView"
        android:layout_below="@+id/dividerUpper" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/exerciseDisplayEntriesLinearLayout"/>
    </ScrollView>


</RelativeLayout>
